generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL") // Le lien de la base de donn√©es 
}

model user {
  id_user     Int     @id @default(autoincrement())
  email       String  @unique
  password    String
}
